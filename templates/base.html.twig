<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}Welcome!{% endblock %}
		</title>
		<script src="https://polyfill.io/v3/polyfill.min.js?features=default"></script>
		{% block stylesheets %}
			{{ ux_controller_link_tags() }}
		{% endblock %}

	</head>
	<body>
		{% block body %}{% endblock %}

        {% block javascripts %}
			<script src="https://maps.googleapis.com/maps/api/js?key={{ GOOGLE_API_KEY }}&libraries=&v=weekly" async></script>
			{% block importmap %}
				{{ importmap('app') }}
			{% endblock %}
		{% endblock %}
	</body>
</html>
